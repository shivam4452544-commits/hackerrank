#include <bits/stdc++.h>
#include <vector>
#include <numeric> // For std::accumulate
#include <algorithm> // For std::find_if, std::not1, ptr_fun

using namespace std;

// --- Utility Functions for Input/Output (Provided in the original prompt) ---

string ltrim(const string &str) {
    string s(str);

    s.erase(
        s.begin(),
        find_if(s.begin(), s.end(), not1(ptr_fun<int, int>(isspace)))
    );

    return s;
}

string rtrim(const string &str) {
    string s(str);

    s.erase(
        find_if(s.rbegin(), s.rend(), not1(ptr_fun<int, int>(isspace))).base(),
        s.end()
    );

    return s;
}

vector<string> split(const string &str) {
    vector<string> tokens;

    string::size_type start = 0;
    string::size_type end = 0;

    while ((end = str.find(" ", start)) != string::npos) {
        tokens.push_back(str.substr(start, end - start));

        start = end + 1;
    }

    tokens.push_back(str.substr(start));

    return tokens;
}

// --- Problem Solving Function ---

/*
 * Complete the 'equalStacks' function below.
 *
 * The function is expected to return an INTEGER.
 * The function accepts following parameters:
 * 1. INTEGER_ARRAY h1
 * 2. INTEGER_ARRAY h2
 * 3. INTEGER_ARRAY h3
 */

int equalStacks(vector<int> h1, vector<int> h2, vector<int> h3) {
    // 1. Calculate the initial total height for each stack.
    // Use long long to prevent potential overflow, although int should suffice for the constraints 
    // typically seen in these problems (heights up to 10^5, number of cylinders up to 10^5).
    long long sum1 = 0, sum2 = 0, sum3 = 0;

    sum1 = accumulate(h1.begin(), h1.end(), 0LL);
    sum2 = accumulate(h2.begin(), h2.end(), 0LL);
    sum3 = accumulate(h3.begin(), h3.end(), 0LL);

    // 2. Initialize indices for the "top" of each stack.
    // We remove cylinders from the top, which corresponds to iterating from the start (index 0) of the vectors.
    int idx1 = 0;
    int idx2 = 0;
    int idx3 = 0;

    // 3. Greedy Iteration: Remove the top cylinder from the tallest stack.
    while (!(sum1 == sum2 && sum2 == sum3)) {
        // A. Check for the impossible case (one stack is empty).
        if (idx1 == h1.size() || idx2 == h2.size() || idx3 == h3.size()) {
            return 0; // Maximum equal height is 0.
        }

        // B. Find the tallest stack and remove its top cylinder.
        if (sum1 >= sum2 && sum1 >= sum3) {
            // Stack 1 is the tallest (or tied for tallest)
            sum1 -= h1[idx1];
            idx1++;
        } else if (sum2 >= sum1 && sum2 >= sum3) {
            // Stack 2 is the tallest (or tied for tallest)
            sum2 -= h2[idx2];
            idx2++;
        } else {
            // Stack 3 is the tallest
            sum3 -= h3[idx3];
            idx3++;
        }
    }

    // 4. Return the result. The heights are now equal.
    return (int)sum1;
}

// --- Main Function (Provided in the original prompt) ---

int main()
{
    // The standard environment for competitive programming often sets up
    // file output this way.
    ofstream fout(getenv("OUTPUT_PATH"));

    string first_multiple_input_temp;
    getline(cin, first_multiple_input_temp);

    vector<string> first_multiple_input = split(rtrim(first_multiple_input_temp));

    int n1 = stoi(first_multiple_input[0]);

    int n2 = stoi(first_multiple_input[1]);

    int n3 = stoi(first_multiple_input[2]);

    string h1_temp_temp;
    getline(cin, h1_temp_temp);

    vector<string> h1_temp = split(rtrim(h1_temp_temp));

    vector<int> h1(n1);

    for (int i = 0; i < n1; i++) {
        int h1_item = stoi(h1_temp[i]);

        h1[i] = h1_item;
    }

    string h2_temp_temp;
    getline(cin, h2_temp_temp);

    vector<string> h2_temp = split(rtrim(h2_temp_temp));

    vector<int> h2(n2);

    for (int i = 0; i < n2; i++) {
        int h2_item = stoi(h2_temp[i]);

        h2[i] = h2_item;
    }

    string h3_temp_temp;
    getline(cin, h3_temp_temp);

    vector<string> h3_temp = split(rtrim(h3_temp_temp));

    vector<int> h3(n3);

    for (int i = 0; i < n3; i++) {
        int h3_item = stoi(h3_temp[i]);

        h3[i] = h3_item;
    }

    int result = equalStacks(h1, h2, h3);

    fout << result << "\n";

    fout.close();

    return 0;
}
